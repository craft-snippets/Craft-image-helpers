{
  "require": {
    "aelvan/imager": "^2.3",
    "craftcms/cms": "^3.4.0",
    "craftsnippets/image-helpers": "^1.0",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "path",
      "url": "C:/xampp/htdocs/image_test/image_helpers"
    }           
  ]
}
