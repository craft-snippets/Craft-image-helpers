{
    "name": "craftsnippets/craft-image-helpers",
    "description": "Image helpers",
    "type": "craft-plugin",
    "version": "1.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "image helpers"
    ],
    "support": {
        "docs": "https://github.com/craft-snippets/Craft-image-helpers/blob/master/README.md",
        "issues": "https://github.com/craft-snippets/Craft-image-helpers/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Piotr Pogorzelski",
            "homepage": "http://craftsnippets.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "craftsnippets\\imagehelpers\\": "src/"
        }
    },
    "extra": {
        "name": "Craft Image helpers",
        "handle": "image-helpers",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "???",
        "components": {
            "imageHelpersService": "craftsnippets\\imagehelpers\\services\\ImageHelpersService"
        },
        "class": "craftsnippets\\imagehelpers\\ImageHelpers"
    }
}
